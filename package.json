{
  "name": "rsg-alt",
  "version": "3.11.0",
  "description": "A React component guide that generates code examples, prop documentation, rendered component samples and active development.",
  "main": "lib/rsg.js",
  "bin": {
    "rsg": "bin/rsg"
  },
  "scripts": {
    "test": "standard && mocha test/**/*.js",
    "start": "NODE_ENV=development node bin/rsg 'example/components/**/*.js' -v -d",
    "clean": "rimraf rsg-tmp/*",
    "styleguide": "NODE_ENV=production node bin/rsg 'example/components/**/*.js' -v",
    "ghpages": "npm run styleguide -- -r 'react-styleguide-generator-alt' && gh-pages -d styleguide"
  },
  "files": [
    ".babelrc",
    "app",
    "bin",
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theogravity/react-styleguide-generator-alt.git"
  },
  "keywords": [
    "component",
    "documentation",
    "documentor",
    "docs",
    "generator",
    "markdown",
    "react",
    "styleguide",
    "react-docgen",
    "style-guide",
    "webpack",
    "sample",
    "examples",
    "guide"
  ],
  "author": "pocotan001",
  "contributors": [
    {
      "name": "ygoto3"
    },
    {
      "name": "Theo Gravity",
      "email": "theo@suteki.nu"
    },
    {
      "name": "Marcello Bastea-Forte",
      "email": "marcello@cellosoft.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theogravity/react-styleguide-generator-alt/issues"
  },
  "homepage": "https://github.com/theogravity/react-styleguide-generator-alt#readme",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist",
      "example"
    ]
  },
  "dependencies": {
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "bunyan": "1.8.1",
    "css-loader": "0.23.1",
    "cssnext": "1.8.4",
    "cssnext-loader": "1.0.1",
    "deepmerge": "anthonator/deepmerge#feature/array-options",
    "express": "4.14.0",
    "fs-extra": "0.24.0",
    "glob": "5.0.15",
    "highlight.js": "9.4.0",
    "immutable": "3.8.1",
    "json-loader": "0.5.4",
    "marked": "0.3.5",
    "minimist": "1.2.0",
    "mustache": "2.2.1",
    "object-assign": "4.1.0",
    "page": "1.7.1",
	"promise": "7.1.1",
    "react-bootstrap": "0.27.1",
    "react-docgen": "2.9.0",
    "react-simpletabs": "matthewgertner/react-simpletabs",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.6",
    "slash": "1.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "devDependencies": {
    "babel-eslint": "4.1.8",
    "eslint-plugin-react": "3.16.1",
    "gh-pages": "0.4.0",
    "mocha": "2.5.3",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "rimraf": "2.4.5",
    "standard": "5.4.1",
    "uglify-js": "2.6.3"
  }
}
